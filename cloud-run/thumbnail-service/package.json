{
  "name": "svelte-wheel-thumbnail-service",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon index.ts",
    "build": "npm run copy-files-to-lib && tsc && npm run copy-files-to-dist",
    "copy-files-to-lib": "npm run copy-utils && npm run copy-server",
    "copy-utils": "cp -r ../../src/lib/utils lib/utils/ || copy ..\\..\\src\\lib\\utils lib\\utils\\",
    "copy-server": "cp -r ../../src/lib/server lib/server/ || copy ..\\..\\src\\lib\\server lib\\server\\",
    "copy-files-to-dist": "npm run copy-docker && npm run copy-package",
    "copy-package": "cp package*.json dist || copy package*.json dist",
    "copy-docker": "cp Dockerfile dist || copy Dockerfile dist",
    "deploy": "npm run build && ./deploy_prod.sh"
  },
  "author": "Gunnar Omander <gunnar@gomander.dev>",
  "license": "ISC",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.44",
    "express": "^4.18.2",
    "firebase": "^10.7.1",
    "firebase-admin": "^12.0.0",
    "module-alias": "^2.2.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "20"
  },
  "_moduleAliases": {
    "$lib": "lib",
    "$env/static/public": "env.js"
  }
}
